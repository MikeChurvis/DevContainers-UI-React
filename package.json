{
  "name": "devcontainers-ui-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:app": "vite",
    "dev:backend": "nodemon --watch './**/*.ts' --exec 'ts-node-esm' ./mock-backend/MockBackend.ts",
    "dev": "run-p --race dev:app dev:backend",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "play": "playwright test --reporter=list",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "eslint": "^8.34.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.1",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint-plugin-react": "^7.32.2",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "mock-socket": "^9.2.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "tailwindcss": "^3.2.6",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.28.5"
  }
}
